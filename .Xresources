URxvt.perl-ext-common: default,url-select
URxvt.url-select.autocopy: true
URxvt.url-select.button: 1
URxvt.url-select.launcher: /usr/bin/firefox
URxvt.url-select.underline: true
URxvt.keysym.M-u: perl:url-select:select_next

URxvt*256-color: true
URxvt*fading: 5
URxvt*colorUL: rgba:0000/4343/7a7a/ffff
URxvt*underlineColor: rgba:aaaa/3333/3333/ffff
URxvt*depth: 32
URxvt*background: rgba:ffff/fffe/fddd/ffff
URxvt*foreground: rgba:0000/0000/0000/ffff
URxvt*transparent: false
URxvt*scrollBar: false
URxvt*font: xft:DejaVu Sans Mono-10
XTerm*termName: xterm-256color
URxvt*termName: rxvt-unicode
